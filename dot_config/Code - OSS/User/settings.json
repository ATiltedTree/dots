{
    "workbench.colorTheme": "Ayu Mirage",
    "editor.fontFamily": "Symbols Nerd Font,monospace",
    "editor.fontLigatures": true,
    "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
    // Change to true when fixed: https://github.com/rust-analyzer/rust-analyzer/issues/8925
    "rust-analyzer.procMacro.enable": false,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": false,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<space>",
                "c",
                "f"
            ],
            "commands": [
                "editor.action.format",
            ]
        },
        {
            "before": [
                "K",
            ],
            "commands": [
                "editor.action.showHover",
            ]
        },
        {
            "before": [
                "<space>",
                "c",
                "d"
            ],
            "commands": [
                "editor.action.goToDeclaration",
            ]
        },
        {
            "before": [
                "<space>",
                "c",
                "a",
            ],
            "commands": [
                "editor.action.quickFix",
            ]
        },
        {
            "before": [
                "<space>",
                "<space>",
            ],
            "commands": [
                "workbench.action.quickOpen",
            ]
        }
    ],
    "vim.hlsearch": true,
    "workbench.iconTheme": "material-icon-theme",
    "editor.linkedEditing": true,
    "editor.rulers": [
        80
    ],
    "editor.smoothScrolling": true,
    "editor.formatOnSave": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "workbench.list.smoothScrolling": true,
    "window.menuBarVisibility": "hidden",
    "rust-analyzer.lens.implementations": false,
    "dart.previewLsp": true,
    "workbench.startupEditor": "newUntitledFile",
    "lldb.library": "/usr/lib/liblldb.so",
    "cSpell.userWords": [
        "Tilmann",
    ],
    "better-comments.tags": [
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "fixme",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "gitlens.codeLens.enabled": false,
    "editor.suggestSelection": "first",
    "dart.debugExternalLibraries": true,
    "dart.debugSdkLibraries": true,
    "editor.minimap.enabled": false,
    "crates.localCargoIndexBranch": "origin/HEAD",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.language": "de,en",
    "redhat.telemetry.enabled": false,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "rust-analyzer.cargo.useRustcWrapperForBuildScripts": false,
    "ltex.java.path": "/usr/lib/jvm/default/",
    "rust-analyzer.diagnostics.disabled": ["unlinked-file"]
}
